<div class="block-s menu-main {{inject.parentClass}}" data-adaptive-width="{{inject.menuBrakePoint}}">
	<div class="block-s__inner menu-main__inner">
		<div class="block-s__controller menu-main__controller">
			<div class="menu-main__controller-icon {{inject.iconSandwichClass}}"></div>

		</div>
		<nav class="block-s__body menu-main__list menu-main__list">
			{% for item in inject.listData %}
				{% include "blocks/menu/" + inject.menuItemTpl + ".njk" %}
			{% endfor %}
		</nav>

	</div>
</div>